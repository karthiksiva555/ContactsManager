@model IEnumerable<PersonResponse>;
@{
    ViewBag.Title = "Persons";
}

<h2>Persons</h2>

<table class="table w-100 mt">
    <thead>
    <tr>
        <th>Person Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Gender</th>
        <th>Country</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.PersonName</td>
                <td>@person.EmailAddress</td>
                <td>@person.Age</td>
                <td>@person.Gender</td>
                <td>@person.Country</td>
            </tr>
        }
    </tbody>
</table>